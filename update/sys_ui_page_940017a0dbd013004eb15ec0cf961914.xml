<?xml version="1.0" encoding="UTF-8"?>
<record_update table="sys_ui_page">
    <sys_ui_page action="INSERT_OR_UPDATE">
        <category>general</category>
        <client_script/>
        <description/>
        <direct>false</direct>
        <endpoint>x_183199_testcommv_ViewClients.do</endpoint>
        <html><![CDATA[<?xml version="1.0" encoding="utf-8" ?>
<j:jelly trim="false" xmlns:j="jelly:core" xmlns:g="glide" xmlns:j2="null" xmlns:g2="null">
	
	<link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous"></link>
	<link href="https://cdnjs.cloudflare.com/ajax/libs/angularjs-toaster/1.1.0/toaster.min.css" rel="stylesheet" />
	<link href="c15d08dddb530300180ad170cf961922.cssdbx" rel="stylesheet" type="text/css"></link> 

	
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
	<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>
	<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.6.4/angular.min.js"></script>
	<script src="//ajax.googleapis.com/ajax/libs/angularjs/1.6.4/angular-sanitize.js"></script>
	<script src="//ajax.googleapis.com/ajax/libs/angularjs/1.6.4/angular-cookies.min.js"></script>
	<script src="https://code.angularjs.org/1.6.4/angular-animate.min.js"></script>
	<script src="https://cdnjs.cloudflare.com/ajax/libs/angularjs-toaster/1.1.0/toaster.min.js"></script>
	<script src="https://code.highcharts.com/stock/highstock.src.js"></script>
	<script src="https://rawgithub.com/pablojim/highcharts-ng/master/src/highcharts-ng.js"></script>
	<g:requires name="x_183199_testcommv.dirPagination.jsdbx"/>
	<script src="x_183199_testcommv.angular_app_client_lo.jsdbx"></script>	
	<script src="x_183199_testcommv.loginDirective.jsdbx"></script>
	<script src="x_183199_testcommv.toaster.svc.jsdbx"></script>
	
	
	<script>
          var $j_commvault = jQuery.noConflict(true);
    </script>
	
	
	<div id="cvapp" ng-app="angularapp" class="ng-cloak">
		
		<div ng-controller="ViewClientsController as vcctrl" class="appContainer" ng-init="getLoginCookie()">
			
			<toaster-container toaster-options="{'position-class': 'toast-bottom-right'}"></toaster-container>
			
			<commvaultlogin callback="getClients()" cv-login="cvLogin" ng-if="unauthorized"></commvaultlogin>
			
			<!-- Backup Modal -->
			
			<div class="modal backupModal fade" id="backupModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
				<div class="modal-dialog" role="document">
					<div class="modal-content">
						<div class="modal-header">
							<button type="button" class="close btn" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&#xD7;</span></button>
							<h4 class="modal-title" id="myModalLabel" ng-if="backupModalOptype === 'BACKUP'"> Backup client</h4>
							<h4 class="modal-title" id="myModalLabel" ng-if="backupModalOptype === 'RESTORE'"> Restore client</h4>
						</div>
						<div class="modal-body">
							<div class="row">
								<div class="col-xs-4 text-center">
									<div class="modal-col-heading">Choose application</div>
									<div class="list-group-container">
										<div class="list-group">
											<a class="list-group-item" ng-class="{'selected': app.selected}" ng-repeat="app in appList" ng-click="selectApp(app)">{{app.appName}}</a>
										</div>
									</div>    
								</div>
								<div class="col-xs-4 text-center">
									<div class="modal-col-heading">Choose backup set</div>
									<div class="list-group-container">
										<div class="list-group">
											<a class="list-group-item" ng-class="{'selected': backupset.selected}" ng-repeat="backupset in backupSetList" ng-click="selectBackupset(backupset)">{{backupset.backupsetName}}</a>
										</div>
									</div> 
								</div>
								<div class="col-xs-4 text-center">
									<div class="modal-col-heading">Chooose sublient</div>
									<div class="list-group-container">
										<div class="list-group">
											<a class="list-group-item" ng-class="{'selected': subclient.selected}" ng-repeat="subclient in subclientList" ng-click="selectSubclient(subclient)">{{subclient.subclientName}}</a>
										</div>
									</div> 
								</div>
							</div>
							
							<div ng-if="backupModalOptype === 'BACKUP'">
								<div class="chooseBackupLevel">
									<div class="row">
										<div class="col-xs-4 text-center">
											<div class="modal-col-heading">Choose backup level</div>
										</div>
									</div>
									<div class="row">
										<div class="col-xs-12">
											<div class="btn-group full-width" role="group">
											  <button type="button" class="btn one-third-width" ng-click="onBackupLevelSelection('FULL')" ng-class="{'selected-level': selectedBackupLevel === 'FULL'}">Full</button>
											  <button type="button" class="btn one-third-width" ng-click="onBackupLevelSelection('INCREMENTAL')" ng-class="{'selected-level': selectedBackupLevel === 'INCREMENTAL'}">Incremental</button>
											  <button type="button" class="btn one-third-width" ng-click="onBackupLevelSelection('DIFFERENTIAL')" ng-class="{'selected-level': selectedBackupLevel === 'DIFFERENTIAL'}" >Differential</button>
											</div>  
										</div>
									</div>
								</div>
							</div>
							
							<div ng-if="backupModalOptype === 'RESTORE'">
								<div class="row">
									<div class="col-xs-4 text-center">
										<div class="modal-col-heading">Choose restore options</div>
									</div>
								</div>
								
								<div class="row">
									<div class="col-xs-12 fsCredentialsContainer margin-top-20">
										<div class="panel panel-default">
										  <div class="panel-heading">Impersonate user (optional)</div>
										  <div class="panel-body">
											  <div class="row margin-top-10">
												<div class="col-xs-12">
													<div class="form-group">
														<input type="text" class="form-control" id="fsusername" name="fsusername" required="true" ng-model="restoreOptions.fsusername"></input>
														<label for="fsusername" class="floating-label">Username</label>
													</div>
												</div>
											  </div>
											  <div class="row margin-top-10">
												<div class="col-xs-12">
													<div class="form-group">
														<input type="password" class="form-control" id="fspassword" name="fspassword" required="true" ng-model="restoreOptions.fspassword"></input>
														<label for="fspassword" class="floating-label">Password</label>
													</div>
												</div>
											  </div>
										  </div>
										</div>
									</div>
								</div>
								
								<div class="row margin-top-20">
									<div class="col-xs-12">
										<div class="form-group">
											<input type="text" class="form-control" id="fsFileSourcePath" name="fsFileSourcePath" required="true" ng-model="restoreOptions.fsFileSourcePath"></input>
											<label for="fsFileSourcePath" class="floating-label">Source path</label>
										</div>
									</div>
								</div>
								<div class="margin-left-10">
									<div class="row">
										<div class="col-xs-12">
											<div class="checkbox">
												<label>
													<input id="fsFileOverwrite" type="checkbox" ng-model="restoreOptions.fsFileOverwrite"> Unconditionally overwrite if it already exists</input>
												</label>
											</div>	
										</div>
									</div>

									<div class="row">
										<div class="col-xs-12">
											<div class="checkbox">
												<label>
													<input id="fsRestoreOrigLocation" type="checkbox" ng-model="restoreOptions.fsRestoreOrigLocation" ng-change="fsRestoreOrigLocationStateChanged()"> Restore to original folder</input>
												</label>
											</div>	
										</div>
									</div>
								</div>	
								
								<div class="row margin-top-20">
									<div class="col-xs-12">
										<div class="form-group">
											<input type="text" class="form-control" id="fsFileDestPath" name="fsFileDestPath" required="true" ng-model="restoreOptions.fsFileDestPath" ng-disabled="restoreOptions.fsRestoreOrigLocation"></input>
											<label for="fsFileDestPath" class="floating-label">Destination path</label>
										</div>
									</div>
								</div>
								
							</div>	
							
							<div class="error-text-container">
								<div class="row">
									<div class="col-xs-12 text-center">
										<span class="error-text">{{errorText}}</span>
										<span class="info-text">{{infoText}}</span>
									</div>
								</div>
							</div>	
							
						</div>
						<div class="modal-footer">
							<button type="button" class="btn actionBtn modalClose" data-dismiss="modal">Close</button>
							<button type="button" class="btn actionBtn" ng-if="backupModalOptype === 'BACKUP'" ng-click="onRunBackup()"> Backup</button>
							<button type="button" class="btn actionBtn" ng-if="backupModalOptype === 'RESTORE'" ng-click="onRunRestore()"> Restore</button>
						</div>
					</div>
				</div>
			</div>

			<!-- Backup Modal End -->
			
			<!-- Activity control modal -->
			
			<div class="modal activityModal fade" id="activityModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
				<div class="modal-dialog" role="document">
					<div class="modal-content">
						<div class="modal-header">
							<button type="button" class="close btn" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&#xD7;</span></button>
							<h4 class="modal-title" id="myModalLabel"> Activity control</h4>
						</div>
						<div class="modal-body">
							
							<div class="row">
								<div class="col-xs-8">
									<div class="actionLabel">
										Data backup
									</div>
								</div>
								<div class="col-xs-4">
									<div class="main-container__column materail-switch materail-switch_primary">
										<input class="materail-switch__element" type="checkbox" id="switch_input1" ng-model="enabledBackup"></input>
										<label class="materail-switch__label" for="switch_input1"></label>
									</div>									
								</div>
							</div>	
							<hr/>
							<div class="row">
								<div class="col-xs-8">
									<div class="actionLabel">
										Data restore
									</div>
								</div>
								<div class="col-xs-4">
									<div class="main-container__column materail-switch materail-switch_primary">
										<input class="materail-switch__element" type="checkbox" id="switch_input2" ng-model="enabledRestore"></input>
										<label class="materail-switch__label" for="switch_input2"></label>
									</div>									
								</div>
							</div>	
							
							<div class="error-text-container">
								<div class="row">
									<div class="col-xs-12 text-center">
										<span class="error-text">{{errorText}}</span>
										<span class="info-text">{{infoText}}</span>
									</div>
								</div>
							</div>	
							
						</div>
						<div class="modal-footer">
							<button type="button" class="btn actionBtn modalClose" data-dismiss="modal">Close</button>
							<button type="button" class="btn actionBtn" ng-click="saveActivitySettings()"> Save</button>
						</div>
					</div>
				</div>
			</div>
			
			<!-- Activity control modal end -->
			
			<div class="row">
				<div class="col-xs-12">
					<div class="form-container">
						<h4 class="imageHeading"><img src="commvault_logo.png" width="300" height="49"/><span class="module-name">View clients</span>
							<div class="cos_load_holder" ng-show="onAjax">
								<div class="cos_load">
									<div class="cos_spinner">
										<div class="cos_mask">
											<div class="cos_maskedCircle"></div>
										</div>
									</div>
								</div>
							</div>			
						</h4>
						<hr/>
						
						
						<div class="row table-action-bar">
							<div class="col-xs-1">
							  <select class="page-size-select" ng-model="pageSize" ng-options="pg for pg in pageSizeList">
							  </select>
							</div>
							
							<div class="col-xs-5">
								<dir-pagination-controls
								   max-size="5"
								   direction-links="true"
								   boundary-links="true"
								   auto-hide="false" >
								</dir-pagination-controls>
							</div>
							
							
							<div class="col-xs-4 col-xs-offset-2">
								<div class="input-group search-term">
								  <input type="text" class="form-control" placeholder="Search" ng-model="searchTerm"></input>
								  <span class="input-group-addon">
										<span class="glyphicon glyphicon-search"></span>
								  </span>
								</div>
							</div>				
						</div>
						
						<div class="row">
							<div class="col-xs-12 text-center">
								<div class="table-container" ng-show="tabeLoaded">
									<table class="table-snow">
										<thead>
											<tr>
												<!--<th>
													<input type="checkbox"></input>
												</th>-->
												<th class="clickable" ng-click="sort('client.clientEntity.displayName')">Client name
													<span class="glyphicon sort-icon" ng-show="sortKey=='client.clientEntity.displayName'" ng-class="{'glyphicon-chevron-up':reverse,'glyphicon-chevron-down':!reverse}"></span>
												</th>
												<th>Client type</th>
												<th class="clickable" ng-click="sort('client.osInfo.OsDisplayInfo.OSName')">Operating system
													<span class="glyphicon sort-icon" ng-show="sortKey=='client.osInfo.OsDisplayInfo.OSName'" ng-class="{'glyphicon-chevron-up':reverse,'glyphicon-chevron-down':!reverse}"></span>
												</th>
												<th class="clickable" ng-click="sort('client.versionInfo.version')">Version
													<span class="glyphicon sort-icon" ng-show="sortKey=='client.versionInfo.version'" ng-class="{'glyphicon-chevron-up':reverse,'glyphicon-chevron-down':!reverse}"></span>
												</th>
												<th class="text-center">Actions</th>
												<!-- <th>Operating System</th>
												<th>Location</th>
												<th>Created By</th> -->
											</tr>
										</thead>
										<tbody class="table-hover">
											<tr dir-paginate="clientProperty in clientResponse.clientProperties | orderBy:sortKey:reverse | filter:searchTerm|itemsPerPage:pageSize">
												<td>{{clientProperty.client.clientEntity.displayName}}</td>
												<td>
													<span ng-repeat="ida in clientProperty.client.idaList">
														{{ida.idaEntity.appName}}<span ng-show=" ! $last ">, </span>
													</span>
												</td>
												<td>{{clientProperty.client.osInfo.OsDisplayInfo.OSName}}</td>
												<td>{{clientProperty.client.versionInfo.version}}</td>
												<td class="text-center">
													<div class="action-dropdown">
														<button role="button" class="btn action-dropbtn" ng-click="toggleActionDropdown(clientProperty)"><span class="glyphicon glyphicon-option-horizontal button-icon"></span></button>
														  <div class="action-dropdown-content" ng-class="{'open':clientProperty.showActionDropdown}">
															<a ng-click="getSubclients(clientProperty.client.clientEntity, 'BACKUP')" class="text-left">Backup</a>
															<a ng-click="getSubclients(clientProperty.client.clientEntity, 'RESTORE')" class="text-left">Restore</a>
															<a ng-click="doActivityControl(clientProperty.client.clientEntity.clientId)" class="text-left">Activity control</a>
														  </div>
													</div>
												</td>
											</tr>
										</tbody>
									</table>
								</div>	
							</div>
						</div>
						
					</div>	
				</div>
			</div>
			
		</div>
	</div>
</j:jelly>]]></html>
        <name>ViewClients</name>
        <processing_script/>
