        <description/>
        <direct>false</direct>
        <endpoint>x_183199_testcommv_ViewClients.do</endpoint>
        <html><![CDATA[<?xml version="1.0" encoding="utf-8" ?>
<j:jelly trim="false" xmlns:j="jelly:core" xmlns:g="glide" xmlns:j2="null" xmlns:g2="null">
	
	
	<link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous"></link>
	<link href="c15d08dddb530300180ad170cf961922.cssdbx" rel="stylesheet" type="text/css"></link> 
	
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
	<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>
	<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.6.4/angular.min.js"></script>
	<script src="https://code.highcharts.com/stock/highstock.src.js"></script>
	<script src="https://rawgithub.com/pablojim/highcharts-ng/master/src/highcharts-ng.js"></script>
	<script src="x_183199_testcommv.angular_app_client_lo.jsdbx"></script>
	
	
	<script>
          var $j_commvault = jQuery.noConflict(true);
    </script>
	
	
	<div id="cvapp" ng-app="angularapp" class="ng-cloak">
		
		
		
		<div ng-controller="ViewClientsController as vcctrl" class="appContainer">
			
			<!-- Backup Modal -->

			<div class="modal backupModal fade" id="backupModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
				<div class="modal-dialog" role="document">
					<div class="modal-content">
						<div class="modal-header">
							<button type="button" class="close btn" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&#xD7;</span></button>
							<h4 class="modal-title" id="myModalLabel"> Backup client</h4>
						</div>
						<div class="modal-body">
							<div class="row">
								<div class="col-xs-4 text-center">
									<div class="modal-col-heading">Choose application</div>
									<div class="list-group-container">
										<div class="list-group">
											<a class="list-group-item" ng-class="{'selected': app.selected}" ng-repeat="app in appList" ng-click="selectApp(app)">{{app.appName}}</a>
										</div>
									</div>    
								</div>
								<div class="col-xs-4 text-center">
									<div class="modal-col-heading">Choose backup set</div>
									<div class="list-group-container">
										<div class="list-group">
											<a class="list-group-item" ng-class="{'selected': backupset.selected}" ng-repeat="backupset in backupSetList" ng-click="selectBackupset(backupset)">{{backupset.backupsetName}}</a>
										</div>
									</div> 
								</div>
								<div class="col-xs-4 text-center">
									<div class="modal-col-heading">Chooose sublient</div>
									<div class="list-group-container">
										<div class="list-group">
											<a class="list-group-item" ng-class="{'selected': subclient.selected}" ng-repeat="subclient in subclientList" ng-click="selectSubclient(subclient)">{{subclient.subclientName}}</a>
										</div>
									</div> 
								</div>
							</div>
							
							<div class="chooseBackupLevel">
								<div class="row">
									<div class="col-xs-4 text-center">
										<div class="modal-col-heading">Choose backup level</div>
									</div>
								</div>
								<div class="row">
									<div class="col-xs-12">
										<div class="btn-group full-width" role="group">
										  <button type="button" class="btn one-third-width" ng-click="onBackupLevelSelection('FULL')" ng-class="{'selected-level': selectedBackupLevel === 'FULL'}">Full</button>
										  <button type="button" class="btn one-third-width" ng-click="onBackupLevelSelection('INCREMENTAL')" ng-class="{'selected-level': selectedBackupLevel === 'INCREMENTAL'}">Incremental</button>
										  <button type="button" class="btn one-third-width" ng-click="onBackupLevelSelection('DIFFERENTIAL')" ng-class="{'selected-level': selectedBackupLevel === 'DIFFERENTIAL'}" >Differential</button>
										</div>  
									</div>
								</div>
							</div>
							
							<div class="error-text-container">
								<div class="row">
									<div class="col-xs-12 text-center">
										<span class="error-text">{{errorText}}</span>
										<span class="info-text">{{infoText}}</span>
									</div>
								</div>
							</div>	
							
						</div>
						<div class="modal-footer">
							<button type="button" class="btn actionBtn modalClose" data-dismiss="modal">Close</button>
							<button type="button" class="btn actionBtn" ng-click="onRunBackup()"> Backup</button>
						</div>
					</div>
				</div>
			</div>

			<!-- Backup Modal End -->
			
			<!-- Activity control modal -->
			
			<div class="modal activityModal fade" id="activityModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
				<div class="modal-dialog" role="document">
					<div class="modal-content">
						<div class="modal-header">
							<button type="button" class="close btn" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&#xD7;</span></button>
							<h4 class="modal-title" id="myModalLabel"> Activity control</h4>
						</div>
						<div class="modal-body">
							
							<div class="row">
								<div class="col-xs-8">
									<div class="actionLabel">
										Data backup
									</div>
								</div>
								<div class="col-xs-4">
									<div class="main-container__column materail-switch materail-switch_primary">
										<input class="materail-switch__element" type="checkbox" id="switch_input1"></input>
										<label class="materail-switch__label" for="switch_input1"></label>
									</div>									
								</div>
							</div>	
							<hr/>
							<div class="row">
								<div class="col-xs-8">
									<div class="actionLabel">
										Data restore
									</div>
								</div>
								<div class="col-xs-4">
									<div class="main-container__column materail-switch materail-switch_primary">
										<input class="materail-switch__element" type="checkbox" id="switch_input2"></input>
										<label class="materail-switch__label" for="switch_input2"></label>
									</div>									
								</div>
							</div>	
							
							<div class="error-text-container">
								<div class="row">
									<div class="col-xs-12 text-center">
										<span class="error-text">{{errorText}}</span>
										<span class="info-text">{{infoText}}</span>
									</div>
								</div>
							</div>	
							
						</div>
						<div class="modal-footer">
							<button type="button" class="btn actionBtn modalClose" data-dismiss="modal">Close</button>
							<button type="button" class="btn actionBtn" ng-click="saveActivitySettings()"> Save</button>
						</div>
					</div>
				</div>
			</div>
			
			<!-- Activity control modal end -->
			
			<div class="row">
				<div class="col-xs-12">
					<div class="form-container">
						<h4 class="imageHeading"><img src="commvault_logo.png" width="300" height="49"/><span class="module-name">View clients</span>
							<div class="cos_load_holder" ng-show="onAjax">
								<div class="cos_load">
									<div class="cos_spinner">
										<div class="cos_mask">
											<div class="cos_maskedCircle"></div>
										</div>
									</div>
								</div>
							</div>			
						</h4>
						<hr/>
						
						<div class="row">
							<div class="col-xs-12 text-center">
								<div class="table-container" ng-show="tabeLoaded">
									<table class="table-snow">
										<thead>
											<tr>
												<!--<th>
													<input type="checkbox"></input>
												</th>-->
												<th>Client name</th>
												<th>Client type</th>
												<th>Operating system</th>
												<th>Version</th>
												<th>Perform backup</th>
												<th>Activity control</th>
												<!-- <th>Operating System</th>
												<th>Location</th>
												<th>Created By</th> -->
											</tr>
										</thead>
										<tbody class="table-hover">
											<tr ng-repeat="clientProperty in clientResponse.clientProperties">
												<td>{{clientProperty.client.clientEntity.displayName}}</td>
												<td>
													<span ng-repeat="ida in clientProperty.client.idaList">
														{{ida.idaEntity.appName}}<span ng-show=" ! $last ">, </span>
													</span>
												</td>
												<td>{{clientProperty.client.osInfo.OsDisplayInfo.OSName}}</td>
												<td>{{clientProperty.client.versionInfo.version}}</td>
												<td>
													<!--<div class="option-btn-container" ng-class="{'open': clientProperty.actionDropdownSelected}">
														  
														  <div class="options-icon-holder">	
															  <span class="glyphicon glyphicon-option-horizontal options-icon option-btn" ng-click="toggleActionDropdown(clientProperty)"></span>	
														  </div>	  
														  <div class="option-btn-dropdown-content">
															<a href="#">Link 1</a>
															<a href="#">Link 2</a>
															<a href="#">Link 3</a>
														  </div>
													</div>-->
													<!--<button type="button" class="btn actionBtn" ng-click="getSubclients(clientProperty.client.clientEntity.clientId)">Backup</button>-->
													<a ng-click="getSubclients(clientProperty.client.clientEntity.clientId)">Backup</a>
												</td>
												<td>
													<a ng-click="doActivityControl(clientProperty.client.clientEntity.clientId)">Activity control</a>
												</td>
											</tr>
										</tbody>
									</table>
								</div>	
							</div>
						</div>
						
					</div>	
				</div>
			</div>
			
		</div>
	</div>
</j:jelly>]]></html>
